{
  "name": "angular-colorful",
  "version": "1.0.0",
  "description": "A tiny color picker component for modern Angular apps",
  "author": "fil0157 <fil0157@ya.ru>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fil0157/angular-colorful.git"
  },
  "homepage": "https://fil0157.github.io/angular-colorful",
  "bugs": {
    "url": "https://github.com/fil0157/angular-colorful/issues"
  },
  "scripts": {
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "ssr:dev": "ng run angular-colorful-app:serve-ssr",
    "ssr:serve": "node dist/app/server/main.js",
    "ssr:build": "ng build --prod && ng run angular-colorful-app:server:production",
    "prerender": "ng run angular-colorful-app:prerender",
    "lib:start": "ng build angular-colorful --watch",
    "lib:build": "ng build angular-colorful --prod && node tools/build-lib.js",
    "lib:test": "ng test angular-colorful",
    "lib:lint": "ng lint angular-colorful",
    "lib:lint:fix": "ng lint angular-colorful --fix",
    "lib:publish": "npm run lib:build && cd dist/lib && npm pack && npm publish",
    "ngcc": "ngcc"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/animations": "~10.2.1",
    "@angular/cli": "~10.2.0",
    "@angular/common": "~10.2.1",
    "@angular/compiler": "~10.2.1",
    "@angular/compiler-cli": "~10.2.1",
    "@angular/core": "~10.2.1",
    "@angular/forms": "~10.2.1",
    "@angular/platform-browser": "~10.2.1",
    "@angular/platform-browser-dynamic": "~10.2.1",
    "@angular/platform-server": "~10.2.1",
    "@angular/router": "~10.2.1",
    "@angular/service-worker": "~10.2.1",
    "@nguniversal/builders": "^10.1.0",
    "@nguniversal/express-engine": "^10.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "express": "^4.17.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "protractor": "~7.0.0",
    "rxjs": "~6.6.0",
    "shelljs": "^0.8.4",
    "ts-node": "~8.3.0",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "zone.js": "~0.10.2"
  },
  "keywords": [
    "angular",
    "color",
    "ngx",
    "picker",
    "colorpicker"
  ]
}
